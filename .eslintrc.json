{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "globals": {
    "angular": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": [
      "error",
      4
    ],
    "quotes": [
      "error",
      "single"
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-cond-assign": [
      "error",
      "always"
    ],
    "no-var": "error",
    "brace-style": [
      "error",
      "1tbs"
    ],
    "no-console": 0,
    "spaced-comment": [
      "error",
      "always"
    ],
    "eol-last": [
      "error",
      "never"
    ],
    "comma-dangle": [
      "error",
      "never"
    ],
    "angular/file-name": 0,
    "function-paren-newline": 0
  }
}